{
    "name": "oleonard/lay",
    "description": "A lite PHP builder meta-framework to get your projects up and running quickly",
    "keywords": ["framework", "lay", "lite framework", "lite php framework", "metaframework", "fast php framework"],
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "homepage": "https://lay.osaitech.dev",
    "time": "2023-12-12",
    "modified": "2023-12-12",
    "autoload": {
        "psr-4": {
            "Oleonard\\Lay\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Osahenrumwen Aigbogun",
            "email": "leonardosahon@gmail.com"
        }
    ],
    "support": {
        "source": "https://github.com/leonardosahon/Lay",
        "issues": "https://github.com/leonardosahon/Lay/issues"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "vlucas/phpdotenv": "^v5.5.0",
        "wolfcast/browser-detection": "2.9.7",
        "phpmailer/phpmailer": "v6.8.0",
        "ext-openssl": "*",
        "ext-mysqli": "*",
        "ext-json": "*",
        "ext-exif": "*",
        "ext-gd": "*",
        "ext-sodium": "*",
        "ext-fileinfo": "*",
        "ext-dom": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "10.3.2"
    },
    "extra": {
        "npm-packages": {
            "terser": "5.15.1",
            "clean-css": "3.4.12"
        }
    }
}
